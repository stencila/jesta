{
  "name": "jesta",
  "softwareVersion": "1.6.0",
  "description": "Stencila plugin for stencils using Node.js",
  "runtimeAlternatives": [
    {
      "kind": "docker",
      "image": "stencila/jesta",
      "args": ["serve"],
      "transport": "stdio",
      "framing": "nld",
      "serialization": "json"
    },
    {
      "kind": "binary",
      "command": "jesta",
      "args": ["serve"],
      "transport": "stdio",
      "framing": "nld",
      "serialization": "json"
    },
    {
      "kind": "package",
      "command": "node",
      "args": ["./dist/src/index.js", "serve"],
      "transport": "stdio",
      "framing": "nld",
      "serialization": "json"
    }
  ],
  "featureList": []
}
